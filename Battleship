# Setup game board
board = [] 

# set up a for loop to go through each of the elements in the outer list and print them
#create 5x5 board (list of list)
for i in range(5):
  board.append(["O"] * 5)

def print_board(board_in):
  for row in board:
  # Remove the commas to turn each row into "0 0 0 0  0" > use .join
     print " ".join(row)
    
print_board(board)

